;;
;; save 6 bytes, 28 cycles
;;

	ld	b,l
	ld	c,h
	ld	hl,_%0
	ld	(hl),b
	inc	hl
	ld	(hl),c
	inc	hl
	ld	(hl),e
	inc	hl
	ld	(hl),d
=
	ld	(_%0),hl
	ld	(_%0 + 2),de
