
;;
;; save 1 byte, 14 cycles
;;

	ld	hl,_%0
	xor	a, a
	ld	(hl), a
	inc	hl
	ld	(hl), a
	inc	hl
	ld	(hl),0x%1
	inc	hl
	ld	(hl),0x%2
=
	ld	hl,0
	ld	(_%0),hl
	ld	hl,0x%2%1
	ld	(_%0 + 2),hl

;;
;; save 3 bytes, 14 cycles
;;

	ld	hl,_%0
	ld	c, (hl)
	inc	hl
	ld	b, (hl)
	push	bc
=
	ld	hl,(_%0)
	push	hl

;;
;; save 3 bytes, 14 cycles
;;

	ld	hl,_%0 + %1
	ld	c, (hl)
	inc	hl
	ld	b, (hl)
	push	bc
=
	ld	hl,(_%0 + %1)
	push	hl

;;
;; save 4 bytes, 18 cycles
;;

	ld	hl,_%0
	ld	a,(hl)
	inc	hl
	ld	h,(hl)
	ld	l,a
	push	hl
=
	ld	hl,(_%0)
	push	hl

;;
;; save 12 bytes, 40 cycles
;;

	push	hl
	ld	a,(_%0)
	ld	(_%1),a
	ld	a,(_%0 + 1)
	ld	(_%1 + 1),a
	ld	a,(_%0 + 2)
	ld	(_%1 + 2),a
	ld	a,(_%0 + 3)
	ld	(_%1 + 3),a
	pop	hl
=
	push	hl
	ld	hl,(_%0)
	ld	(_%1),hl
	ld	hl,(_%0 + 2)
	ld	(_%1 + 2),hl
	pop	hl

